{
    "name": "no-deps-scripted",
    "version": "1.0.0",
    "scripts": {
        "preinstall": "echo 'module.exports.push(100);' >> log.js",
        "install": "echo 'module.exports.push(200);' >> log.js",
        "postinstall": "echo 'module.exports.push(300);' >> log.js; echo 'module.exports = '\"$RANDOM\"';' > rnd.js"
    }
}
